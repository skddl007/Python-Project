   "source": [
    "# 3. Given a positive integer, find out its factorial."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "factorial of the + integer: 24\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"enter a + integer\"))\n",
    "\n",
    "# Initialize the factorial variable\n",
    "fact = 1\n",
    "\n",
    "# for factorial we have need of range up to this no.\n",
    "for i in range(1,n+1):\n",
    "    fact = fact*i\n",
    "\n",
    "print(\"factorial of the entered integer:\", fact)\n"
   ]
  }
